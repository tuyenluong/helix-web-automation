<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
    <suite name="All Test Suite" parallel="methods" thread-count="2">
    <listeners>
        <listener class-name="ioc.listeners.TestListener"></listener>
        <listener class-name="ioc.listeners.SuiteListener"></listener>
    </listeners>
        <test verbose="2" name="selenium-reflection">
            <classes>
                <class name="tests.tests.UsingMultipleDriverForEachTestMethod">
                    <methods>
                        <include name="testTitle1"/>
<!--                        <include name="testTitle2"/>-->
                    </methods>
                </class>
                <class name="tests.tests.UsingOneDriverForOneUsingMultipleDriverForEachTestMethodClass">
                    <methods>
                        <include name="testTitle1"/>
<!--                        <include name="testTitle2"/>-->
                    </methods>
                </class>
            </classes>
        </test>
</suite>